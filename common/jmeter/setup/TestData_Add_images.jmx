          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor" enabled="true">
            <stringProp name="TestPlan.comments">Write location header to a csv</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">import java.io.FileWriter;
import java.io.BufferedReader;

try {
	String [] headers = prev.getResponseHeaders().split(System.getProperty(&quot;line.separator&quot;));
	String imageId = null;
	for (int i = 0; i &lt; headers.length; i++) {
  		String header = headers[i];
  		
  		if (header.startsWith(&quot;Location&quot;)) {
  			
  			String locationUrl = header.split(&quot;:&quot;,2)[1].trim();
  			
  			int lastIndex = locationUrl.lastIndexOf(&quot;/&quot;);
  			imageId = locationUrl.substring(lastIndex+1);
  			log.info(imageId);
  			
      		//vars.put(&quot;HEADER&quot;, header.split(&quot;:&quot;,2)[1].trim());
      		break;
  		}
	}
	
	String filePath = vars.get(&quot;locationCsvPath&quot;);
	
	FileWriter fstream = new FileWriter(filePath, true);
	
	BufferedWriter out = new BufferedWriter(fstream);
	
	out.write(imageId);
	
	out.write(&quot;\n&quot;);
	out.close();
	fstream.close();
}
catch (Throwable ex) {
   log.info(&quot;Error in Beanshell&quot;, ex);
   throw ex;
}</stringProp>
          </BeanShellPostProcessor>
